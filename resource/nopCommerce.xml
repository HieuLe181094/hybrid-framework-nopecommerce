<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >

<suite name="Nopcommerce UI Automation" verbose="1" parallel="tests" thread-count="2" >

    <listeners>
        <listener class-name="commons.MethodListener"/>
    </listeners>

    <parameter name="environment" value="dev"/>


    <test name="Run on Firefox browser" >
        <parameter name="browser" value="firefox"/>
        <parameter name="osName" value="windows"/>
        <parameter name="ipAddress" value="192.168.0.163"/>
        <parameter name="port" value="5555"/>
        <classes>
            <class name="com.nopcommerce.users.Level_27_GRID"/>
        </classes>
    </test>

    <test name="Run on Chrome browser" >
        <parameter name="browser" value="chrome"/>
        <parameter name="osName" value="windows"/>
        <parameter name="ipAddress" value="192.168.0.163"/>
        <parameter name="port" value="5555"/>
        <classes>
            <class name="com.nopcommerce.users.Level_27_GRID"/>
        </classes>
    </test>

</suite>